#!/usr/bin/env node

'use strict';

var config = require('acm'),
    varname = process.argv[2];

varname && console.log('var %s =', varname);

console.log({
    embedly: { key: config.get('embedly.api.key') },
    firebase: { url: config.get('firebase.url') },
    locate: { dateFormat: config.get('locale.dateFormat') }
});

varname && console.log(';');
