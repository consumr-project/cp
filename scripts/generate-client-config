#!/usr/bin/env node

'use strict';

var config = require('acm'),
    varname = process.argv[2];

if (varname) {
    console.log('var %s =', varname);
}

console.log({
    embedly: {
        key: config.get('embedly.api.key')
    },
    firebase: {
        url: config.get('firebase.url')
    },
    auth: {
        url: config.get('session.auth.url')
    }
});

if (varname) {
    console.log(';');
}
